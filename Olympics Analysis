    "# which olympics athelete has more gold medals in summer\n",
    "goldWinner=summerWinner[summerWinner['medal']=='Gold']['athlete'].value_counts().nlargest(20).reset_index()\n",
    "goldWinner.columns=['Name','Gold medals']\n",
